<!-- nav Bar  -->
<%- include("./Boilerplate/navbar.ejs") %>


<div class="resume_outer template_outer">
     <div class="resume_template finalResume">

    <% if(resume) { %>
        <!-- Blue Section start-->
        <div class="blue_sec">
            
            <div class="template_pic">
              <% if(resume.URL === "") { %>
                 <img src="data:image/png;base64," alt="">
               <% } else { %>
                  <img src="<%=resume.URL%>" alt="Profile_image">
                <% } %>
            </div>

            <div class="template_name">
                <h2> <%= resume.name %> </h2>
            </div>
            
                <!-- Contact Sec  -->
                <div class="contact_sec">
                    <div class="contact_sec_heading">
                      Contact
                    </div>

                    <!-- Address  -->
                    <div class="small_heading">
                      Address
                    </div>

                    <p class="small_heading_light"> <%= resume.city %> , <%= resume.state %> </p>

                    <!-- Phone -->
                    <div class="small_heading">
                      Phone
                    </div>

                    <p class="small_heading_light"> <%= resume.phone %> </p>


                     <!-- Email -->
                    <div class="small_heading">
                      Email
                    </div>

                    <p class="small_heading_light"> <span style="font-size: 10px"> <%= resume.email %> </span> </p>

                </div>


                
                <!-- Skill Section  -->
                <% if(resume.skills)  { %>
                  <div class="skill_sec white">
                      <div class="contact_sec_heading">
                          Skills
                      </div>
                
                      <% let skills = resume.skills.toString().split(","); %>
                      <% for(skill in skills) { %>
                        <p class="small_heading_light"> <%= skills[skill] %> </p>
                      <% } %>
                  </div>
               <% } %>
       </div>
       <!-- Blue Section end -->

       <!-- White sec start -->
        <div class="white_sec">
             
        
          <!-- Experiance  -->
          <% if(resume.skills)  { %>
              <div class="white_sec_heading">
                <hr>
                <h4> Experiance </h4>
                <hr>
              </div>

              <% let experiance = resume.Experiance.toString().split(","); %>
              <% let exp_month = resume.Experiance_month.toString().split(","); %>
              <% for(exp in experiance) { %>
                <div class="white_sec_data">
                    <div class="white_sec_data_left">
                        <%= exp_month[exp] %> <span style="font-size: 12px">months</span>
                    </div>

                    <div class="white_sec_data_right">
                        <%= experiance[exp] %>
                    </div>
                </div>
              <% } %>
            <% } %>
            
            <!-- Education  -->
            <div class="white_sec_heading">
              <hr>
              <h4> Education </h4>
              <hr>
            </div>

            <div class="white_sec_data">
              <div class="white_sec_data_left">
                   School Name
              </div>

              <div class="white_sec_data_right">
                  <%= resume.school %>
              </div>
            </div>

              <% if(resume.university) { %>
                 
                <% let universities = resume.university.toString().split(",") %>
                <% for(uni in universities)  { %>

                  <div class="white_sec_data">
                    <div class="white_sec_data_left">
                         University
                    </div>
      
                    <div class="white_sec_data_right">
                      <%=  universities[uni] %>
                    </div>
                  </div>
                      
                  
                <% } %>

              <% } %>
           


           <!-- Achievements  -->
          <% if(resume.certificate)  { %>
            <div class="white_sec_heading">
              <hr>
              <h4> Achievements </h4>
              <hr>
            </div>

            <% let certificate = resume.certificate.toString().split(","); %>
            <% for(certi in certificate) { %>

                <div class="white_sec_data">
                  <div class="white_sec_data_left">
                      Certificate
                  </div>

                  <div class="white_sec_data_right">
                      <%= certificate[certi] %>
                  </div>
                </div>
            <% } %>
          <% } %>
           
        </div>
        <!-- White sec end -->

    <% } %>
     </div>
 </div>         
          


        
       

    <div class="download-btn">
      <button class="btn btn-success" id="download" onclick="printScreen()"> Download Resume </button>
    </div>

    <div class="space">

    </div>


    <script>
      
        function printScreen() {
          print();
        }
    </script>
   
<!-- footer  -->
<%- include("./Boilerplate/footer.ejs") %>